generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model WatchedFolder {
  id         String   @id @default(cuid())
  userId     String
  folderId   String
  folderName String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@unique([userId, folderId])
}
